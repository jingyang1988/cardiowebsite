<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui">
<ui:composition template="WEB-INF/templates/MasterPage.xhtml">
	<ui:define name="title">Query Patient</ui:define>
	<ui:define name="content">
		<h:form id="queryPatient">
			<br />
			<table width="90%" align="center">
				<tr>
					<td align="left"><p:commandButton value="Back" style="background-color:#A1CAF1;" action="QueryDatabase.xhtml" ajax="false"></p:commandButton></td>
					<td align="right"><p:commandButton value="Log Out" style="background-color:#FFBF00;"
							action="#{loginBean.logOut()}" ajax="false"></p:commandButton></td>
				</tr>
			<tr>
				<td width="90%" align="center" colspan="2">
				
				<h:inputText value="#{queryBean.physicianName}" />
				<h:commandButton value="Add"  type="submit" ajax="false" action="ConfirmAddPhysician.xhtml"
					styleClass="command-button" style="background-color:#8580b1;" />
				</td>
				</tr>
			</table>

		</h:form>
	</ui:define>
</ui:composition>
</html>