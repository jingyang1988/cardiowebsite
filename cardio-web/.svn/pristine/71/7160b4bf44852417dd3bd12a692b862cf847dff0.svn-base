<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui">
<ui:composition template="WEB-INF/templates/MasterPage.xhtml">
	<ui:define name="title">Patient Data</ui:define>
	<ui:define name="content">
		<h:form id="patienData">
			<br/>
			<table width="90%" align="center">
				<tr>
				
				<ui:repeat var="page" value="mainMenu.xhtml">
					<td align="left"><p:commandButton value="Back" style="background-color:#A1CAF1;"
							action="#{pageLogBean.logger(page)}" ajax="false"></p:commandButton></td>
				</ui:repeat>			
							
				    <td align="center"><p:commandButton value="Save Changes" style="background-color:#FF9966;" action="#{patientDataBean.savePatientData()}" ajax="false"></p:commandButton></td>
					<td align="right"><p:commandButton value="Log Out" style="background-color:#FFBF00;"
							action="#{loginBean.logOut()}" ajax="false"></p:commandButton></td>
				</tr>
				<tr> 
				<p:messages id="messages" showDetail="true" autoUpdate="true" closable="true" /> </tr>
				
			</table>
			<br />
			
			<!--  <table width="90%" align="center">
	    		<tr>
	    			<td align="right"><p:commandButton value="Save Changes" action="#{patientDataBean.savePatientData()}" ajax="false"></p:commandButton></td>
	    		</tr>
	    		</table> -->
	    	<br /> 	
	    	
			<p:panel id="patientData" style="border:0;">
				<table align="center" width="80%">
					<tr>
						<td align="right" width="10%"><p:outputLabel
								value="Patient&nbsp;Name:"></p:outputLabel></td>
						<td align="left" width="10%"><p:inputText
								value="#{patientDataBean.patientData.name}"
								style="color:black;font-weight:bold;"></p:inputText></td>
						<td align="right" width="7%"><p:outputLabel value="MRN:"></p:outputLabel></td>
						<td align="left" width="7%"><p:inputText
								value="#{patientDataBean.patientData.mrn}"
								style="color:black;font-weight:bold;"></p:inputText></td>
						<td align="right" width="10%"><h:outputLabel
								value="WorkUp&nbsp;Status:"></h:outputLabel></td>
						<td align="left" width="7%"><p:selectOneMenu  rendered="#{mainMenuBean.selectedPatient!=null}" value="#{mainMenuBean.selectedPatient.workUpId}">
								<f:selectItems value="#{utilBean.workupList}"/>
								<f:ajax event="change" listener="#{mainMenuBean.updateSeachedPatient}"></f:ajax>
							</p:selectOneMenu></td>
						<td align="right" width="7%"><h:outputLabel
								value="LCEC&nbsp;Status:"></h:outputLabel></td>
						<td align="left" width="7%"><p:selectOneMenu rendered="#{mainMenuBean.selectedPatient!=null}" value="#{mainMenuBean.selectedPatient.lcecId}">
								<f:selectItems value="#{utilBean.lcecStatusList}"/>
								<f:ajax event="change" listener="#{mainMenuBean.updateSeachedPatient}"></f:ajax>
							</p:selectOneMenu></td>
					</tr>
				</table>
			</p:panel>
				<br/>
				<table width="60%" align="center">
				<tr>
					<td align="right"><p:outputLabel value="SSN:"></p:outputLabel></td>
					<td align="left"><p:inputMask mask="999-99-9999" value="#{patientDataBean.patientData.ssn}" /></td>
					<td align="right"><p:outputLabel value="Gender:"></p:outputLabel></td>
					<td align="left"><p:selectOneMenu value="#{patientDataBean.patientData.gender}">
							<f:selectItems value="#{utilBean.genderList}" />
							<f:ajax event="change" listener="#{mainMenuBean.saveChanges}"></f:ajax>
						</p:selectOneMenu></td>
					<td align="right"><p:outputLabel value="Date of Birth:"></p:outputLabel></td>
					<td align="left"><p:calendar id="calDOB" effect="slideDown" value="#{patientDataBean.patientData.dob}"
							navigator="true"></p:calendar></td>
				</tr>
			</table>
			<br />
			
			
			<br />
	<table width="90%" align="center">
		<tr>
			<td>
			<p:fieldset legend="Address / Contact Information">
				<br />
				<table width="100%" align="center">
					<tr>
						<td align="right" width="10%"><h:outputLabel value="Address:"></h:outputLabel></td>
						<td align="left" width="10%"><p:inputText id="txtAddress" value="#{patientDataBean.patientData.address}"> <f:ajax event="change" listener="#{mainMenuBean.saveChanges}"></f:ajax></p:inputText></td>
						<td align="right" width="10%"><h:outputLabel value="City:"></h:outputLabel></td>
						<td align="left" width="10%"><p:inputText id="txtCity" value="#{patientDataBean.patientData.city}"></p:inputText></td>
						<td align="right" width="10%"><h:outputLabel value="State/Province:"></h:outputLabel></td>
						<td align="left" width="10%"><p:inputText id="txtState" value="#{patientDataBean.patientData.state}"></p:inputText></td>
						<td align="right" width="10%"><h:outputLabel value="Postal&nbsp;Code:"></h:outputLabel></td>
						<td align="left" width="10%"><p:inputText id="txtPostalCode" value="#{patientDataBean.patientData.pincode}" ></p:inputText></td>
					</tr>
					<tr>
						<td align="right" width="10%"><h:outputLabel value="Phone&nbsp;Number:"></h:outputLabel></td>
						<td align="left" width="10%"><p:inputMask value="#{patientDataBean.patientData.phoneNum}" mask="(999) 999-9999" /></td>
						<td align="right" width="10%"><h:outputLabel
								value="Alternate&nbsp;Number:"></h:outputLabel></td>
						<td align="left" width="10%"><p:inputMask value="#{patientDataBean.patientData.altPhoneNum}" mask="(999) 999-9999" /></td>
						<td align="right" width="10%"><h:outputLabel value="e-mail:"></h:outputLabel></td>
						<td align="left" width="10%"><p:inputText id="txtEmail" value="#{patientDataBean.patientData.email}"></p:inputText></td>
					</tr>
				</table>
				<br />
			</p:fieldset>
			<br/>
		</td>
	</tr>
	<tr>
		<td>
			<p:fieldset legend="Insurance Information">
				<br />
				<table width="100%" align="center">
					<tr>
						<td align="right" width="10%"><h:outputLabel
								value="Insurance&nbsp;Company:"></h:outputLabel></td>
						<td align="left" width="10%"><p:selectOneMenu value="#{patientDataBean.patientData.insuranceCompany}" style="width:90%;">
								<f:selectItems value="#{utilBean.insuranceList}" />
							</p:selectOneMenu></td>
						<td align="right" width="10%"><h:outputLabel
								value="Insurance&nbsp;ID&nbsp;#:"></h:outputLabel></td>
						<td align="left" width="10%"><p:inputText id="txtInsuranceId" value="#{patientDataBean.patientData.insuranceId}" style="width:90%;"></p:inputText></td>
						<td align="right" width="10%"><h:outputLabel
								value="Insurance&nbsp;Phone:"></h:outputLabel></td>
						<td align="left" width="10%"><p:inputMask value="#{patientDataBean.patientData.insurancePhoneNum}" mask="(999) 999-9999"  style="width:90%;"/></td>
						<td align="right" width="10%"><h:outputLabel value="Comment:"></h:outputLabel></td>
						<td align="left" width="10%"><p:inputText id="txtComment" value="#{patientDataBean.patientData.insuranceComment}" style="width:90%;" ></p:inputText></td>
					</tr>
				</table>
				<br />
			</p:fieldset>
			<br/>
		</td>
	</tr>
	<tr>
		<td>
			<p:fieldset legend="Referral Information">
				<br />
				<table width="100%" align="center">
					<tr>
						<td align="right" width="10%"><h:outputLabel
								value="Date&nbsp;of&nbsp;Referral:"></h:outputLabel></td>
						<td align="left" width="10%"><p:calendar id="calDateRef"
								effect="slideDown" navigator="true" value="#{patientDataBean.patientData.referralDate}" style="width:10%;"/></td>
						<td width="10%" align="center"><p:outputLabel value="Referring MD" 
							for="searchByName" /></td> <td width="15%"> <p:autoComplete id="searchByName"
							minQueryLength="1" effect="fade"
							value="#{patientDataBean.patientData.referringMD}"
							completeMethod="#{utilBean.getRefMDListByName}" var="md"
							itemLabel="#{md}"
							itemValue="#{md}" 
							forceSelection="true">
							
						</p:autoComplete></td>
						<td width="10%" align="center"><p:outputLabel value="Primary Care" 
							for="searchByName1" /></td> <td width="15%"> <p:autoComplete id="searchByName1"
							minQueryLength="1" effect="fade"
							value="#{patientDataBean.patientData.primaryCare}"
							completeMethod="#{utilBean.getRefMDListByName}" var="md"
							itemLabel="#{md}"
							itemValue="#{md}" 
							forceSelection="true">
							
						</p:autoComplete></td>
						
							<td width="10%" align="center"><p:outputLabel value="Additional MD" 
							for="searchByName2" /></td> <td width="15%"> <p:autoComplete id="searchByName2"
							minQueryLength="1" effect="fade"
							value="#{patientDataBean.patientData.additionalMD}"
							completeMethod="#{utilBean.getRefMDListByName}" var="md"
							itemLabel="#{md}"
							itemValue="#{md}" 
							forceSelection="true">
							
						</p:autoComplete></td>
						
					</tr>
				</table>
				<br />
			</p:fieldset>
			<br />
		</td>
	</tr>
	<tr>
		<td>
			<p:fieldset legend="Status">
				<br />
				<table width="100%" align="center">
					<tr>
						<td align="left" width="10%"><p:selectBooleanCheckbox value="#{patientDataBean.patientData.lcecChart}" itemLabel="LCEC&nbsp;Chart:"/></td>
						<td align="right" width="10%"><h:outputLabel value="WorkUp&nbsp;Status:"></h:outputLabel></td>
						<td align="left" width="10%"><p:selectOneMenu value="#{patientDataBean.patientData.workUpId}" style="width:100%">
								<f:selectItems value="#{utilBean.workupList}" />
							</p:selectOneMenu></td>
						<td align="right" width="10%"><h:outputLabel value="LCEC&nbsp;Status:"></h:outputLabel></td>
						<td align="left" width="10%"><p:selectOneMenu value="#{patientDataBean.patientData.lcecId}" style="width:100%">
								<f:selectItems value="#{utilBean.lcecStatusList}"/>
							</p:selectOneMenu></td>
						<td align="right" width="10%"><p:outputLabel value="Inactive&nbsp;Date:"></p:outputLabel></td>
						<td align="left" width="10%"><p:calendar id="calInactiveDate" value="#{patientDataBean.patientData.inactiveDate}"
								effect="slideDown" navigator="true" /></td>
						<td align="left" width="15%"><p:selectBooleanCheckbox value="#{patientDataBean.patientData.scheduledMD}" itemLabel="Schedule&nbsp;F/U's&nbsp;with&nbsp;MD:"/></td>
					</tr>
				</table>
				<br />
			</p:fieldset>
			<br/>
		</td>
	</tr>
	</table>
			<br />
			<br />
