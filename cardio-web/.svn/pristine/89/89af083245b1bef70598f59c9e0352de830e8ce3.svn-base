package lcec.model;

public class Bronch implements java.io.Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	int biopsyAutoNum;
	int bronchAutoNum;
	int bronchType;
	String location;
	int rose;
	int numberPasses;
	int lymph;
	String biopsyResult;
	String comment;
	String diagnostic;
	String accuracy;
	String culture;
	boolean EBUS;
	boolean EMN;
	
	public Bronch() {
		this.bronchAutoNum = -1;
	
	}
	public boolean getEBUS() {
		return EBUS;
	}

	public void setEBUS(boolean eBUS) {
		EBUS = eBUS;
	}

	public boolean getEMN() {
		return EMN;
	}

	public void setEMN(boolean eMN) {
		EMN = eMN;
	}
	public int getBronchAutoNum() {
		return bronchAutoNum;
	}
	public void setBronchAutoNum(int bronchAutoNum) {
		this.bronchAutoNum = bronchAutoNum;
	}
	public int getBiopsyAutoNum() {
		return biopsyAutoNum;
	}
	public void setBiopsyAutoNum(int biopsyAutoNum) {
		this.biopsyAutoNum = biopsyAutoNum;
	}
	public int getBronchType() {
		return bronchType;
	}
	public void setBronchType(int bronchType) {
		this.bronchType = bronchType;
	}
	public String getLocation() {
		return location;
	}
	public void setLocation(String location) {
		this.location = location;
	}
	public int getRose() {
		return rose;
	}
	public void setRose(int rose) {
		this.rose = rose;
	}
	public int getNumberPasses() {
		return numberPasses;
	}
	public void setNumberPasses(int numberPasses) {
		this.numberPasses = numberPasses;
	}
	public int getLymph() {
		return lymph;
	}
	public void setLymph(int lymph) {
		this.lymph = lymph;
	}
	public String getBiopsyResult() {
		return biopsyResult;
	}
	public void setBiopsyResult(String biopsyResult) {
		this.biopsyResult = biopsyResult;
	}
	public String getComment() {
		return comment;
	}
	public void setComment(String comment) {
		this.comment = comment;
	}
	public String getDiagnostic() {
		return diagnostic;
	}
	public void setDiagnostic(String diagnostic) {
		this.diagnostic = diagnostic;
	}
	public String getAccuracy() {
		return accuracy;
	}
	public void setAccuracy(String accuracy) {
		this.accuracy = accuracy;
	}
	public String getCulture() {
		return culture;
	}
	public void setCulture(String culture) {
		this.culture = culture;
	}
	
}
