package lcec.model;

import java.io.Serializable;
import java.util.Date;

public class TestingReportEntry implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	Patient patient;
	PatientTesting test;
	
	public TestingReportEntry(){
		this.patient = new Patient();
		this.test = new PatientTesting();
	}
	
	public String getName() {
		return this.patient.getName();
	}

	public String getMrn() {
		return this.patient.getMrn();
	}

	public String getWorkupStatus() {
		return this.patient.getWorkupStatus();
	}

	public String getLcecStatus() {
		return this.patient.getLcecStatus();
	}
	
	public int getWorkUpId() {
		return this.patient.getWorkUpId();
	}

	public int getLcecId() {
		return this.patient.getLcecId();
	}	
	
	public void setMrn(String mrn) {
		this.patient.setMrn(mrn);
	}
	
	public void setName(String name) {
		this.patient.setName(name);
	}
	
	public void setWorkupStatus(String workupStatus) {
		this.patient.setWorkupStatus(workupStatus);
	}
	
	public void setLcecStatus(String lcecStatus) {
		this.patient.setLcecStatus(lcecStatus);
	}

	public void setWorkUpId(int workUpId) {
		this.patient.setWorkUpId(workUpId);
	}

	public void setLcecId(int lcecId) {
		this.patient.setLcecId(lcecId);
	}
	
	
	public String getMRN() {
		return this.test.getMRN();
	}
	public void setMRN(String mRN) {
		this.test.setMRN(mRN);
	}
	public int getID() {
		return this.test.getID();
	}
	public void setID(int iD) {
		this.test.setID(iD);
	}	
	public String getTestTypeName() {
		return this.test.getTestTypeName();
	}
	public void setTestTypeName(String testTypeName) {
		this.test.setTestTypeName(testTypeName);
	}	
	public int getTestType() {
		return this.test.getTestType();
	}
	public void setTestType(int testType) {
		this.test.setTestType(testType);
	}
	public Date getRequiredDate() {
		return this.test.getRequiredDate();
	}
	public void setRequiredDate(Date requiredDate) {
		this.test.setRequiredDate(requiredDate);
	}
	public Date getTestingDate() {
		return this.test.getTestingDate();
	}
	public void setTestingDate(Date testingDate) {
		this.test.setTestingDate(testingDate);
	}
	public String getComment() {
		return this.test.getComment();
	}
	public void setComment(String comment) {
		this.test.setComment(comment);
	}
	public Boolean getOrderByLCEC() {
		return this.test.getOrderByLCEC();
	}
	public void setOrderByLCEC(Boolean orderByLCEC) {
		this.test.setOrderByLCEC(orderByLCEC);
	}
}
