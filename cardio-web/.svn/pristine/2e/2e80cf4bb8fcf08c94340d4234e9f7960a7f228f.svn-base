package lcec.model;

import java.io.Serializable;
import java.util.Date;

public class PatientListModel implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;


	public PatientListModel() {
		// TODO Auto-generated constructor stub
	}
	
	public PatientListModel(String mrn,String name,Date referralDate,int noOfProcedures,String referringMD,String primaryCarePhysician){
		this.mrn = mrn;
		this.name = name;
		this.referralDate = referralDate;
		this.noOfProcedures = noOfProcedures;
		this.referringMD = referringMD;
		this.primaryCarePhysician = primaryCarePhysician;
	}
	
	
	private String mrn;
	private String name;
	private Date referralDate;
	private int noOfProcedures;
	private String referringMD;
	private String primaryCarePhysician;
	

	public String getMrn() {
		return mrn;
	}

	public void setMrn(String mrn) {
		this.mrn = mrn;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Date getReferralDate() {
		return referralDate;
	}
	public void setReferralDate(Date referralDate) {
		this.referralDate = referralDate;
	}
	public int getNoOfProcedures() {
		return noOfProcedures;
	}
	public void setNoOfProcedures(int noOfProcedures) {
		this.noOfProcedures = noOfProcedures;
	}
	public String getReferringMD() {
		return referringMD;
	}
	public void setReferringMD(String referringMD) {
		this.referringMD = referringMD;
	}
	public String getPrimaryCarePhysician() {
		return primaryCarePhysician;
	}
	public void setPrimaryCarePhysician(String primaryCarePhysician) {
		this.primaryCarePhysician = primaryCarePhysician;
	}
	
	

}
