<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui">
<ui:composition template="WEB-INF/templates/MasterPage.xhtml">
	<ui:define name="title">rptPreOps</ui:define>
	<ui:define name="content">
		<h:form id="queryPatient">
			<br />
			<table width="30%" align="center">
				<tr>
				
				<ui:repeat var="page" value="Reports.xhtml">
					<td align="left"><p:commandButton value="Back" style="background-color:#A1CAF1;" action="#{pageLogBean.logger(page)}"></p:commandButton></td>
				</ui:repeat>				
				
					<td align="right"><p:commandButton value="Log Out" style="background-color:#FFBF00;"
							action="#{loginBean.logOut()}" ajax="false"></p:commandButton></td>
				</tr>
			<tr>
				<td width="90%" align="center" colspan="2">
				
			<br />

			<p:dataTable var="p" paginator="true" rows="25"  
                 paginatorTemplate="{CurrentPageReport}  {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"  
                 rowsPerPageTemplate="25,50,100,#{queryBean.rptPreOps.size()}"
				value="#{queryBean.rptPreOps}" >

				<p:column  headerText="MRN" width="3%" styleClass="wrap">
					<h:outputText value="#{p.MRN}" />
				</p:column>

				

				<p:column headerText="Name" width="2%" styleClass="wrap">
					<h:outputText value="#{p.name}"></h:outputText>
				</p:column>

				
				<p:column headerText="Workup Status" width="2%" styleClass="wrap">
					<h:outputText value="#{p.workupstatus}"></h:outputText>
									</p:column>
									
									<p:column headerText="Phone Number" width="2%" styleClass="wrap">
					<h:outputText value="#{p.phonenumber}"></h:outputText>
					</p:column>
			</p:dataTable>
				</td>
				</tr>
			</table>
			

		</h:form>
	</ui:define>
</ui:composition>
</html>