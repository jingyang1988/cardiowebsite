<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui">
<ui:composition template="WEB-INF/templates/MasterPage.xhtml">
	<ui:define name="title">Add new Referring MD</ui:define>
	<ui:define name="content">
		<h:form id="queryPatient">
			<br />
			<table width="90%" align="center">
				<tr>
					<td align="left"><p:commandButton value="Back" style="background-color:#A1CAF1;" action="AddreferringMD.xhtml" ajax="false"></p:commandButton></td>
					<td align="right"><p:commandButton value="Log Out" style="background-color:#FFBF00;"
							action="#{loginBean.logOut()}" ajax="false"></p:commandButton></td>
				</tr>
				<tr> 
				<p:messages id="messages" showDetail="true" autoUpdate="true" closable="true" /> </tr>
				</table>
				<h:panelGrid columns="2" width="100%" styleClass="panel-center"
				style="text-align: center;">
					

	
				Physician name:<p:inputText id="Referring" value="#{referPhysicianBean.referring}" /><br />
				Title:<p:inputText id="Title" value="#{referPhysicianBean.title}" /><br />
				Groupname:<p:inputText id="Groupname" value="#{referPhysicianBean.groupname}" /><br />
				Address:<p:inputText id="Address" value="#{referPhysicianBean.address}" /><br />
				City:<p:inputText id="City" value="#{referPhysicianBean.city}" /><br />
				State:<p:inputText id="State" value="#{referPhysicianBean.state}" /><br />
				Zip:<p:inputText id="Zip" value="#{referPhysicianBean.zip}" /><br />
				Office:<p:inputText id="Office" value="#{referPhysicianBean.office}" /><br />
				Fax:<p:inputText id="Fax" value="#{referPhysicianBean.fax}" /><br />
				<p:commandButton value="Add"  type="submit" ajax="false" action="#{referPhysicianBean.addReferPhysician}" style="background-color:#8580b1;" />
				</h:panelGrid>
		</h:form>
	</ui:define>
</ui:composition>
</html>