package lcec.model;

import java.io.Serializable;


public class PatientBronch implements Serializable{
	
	private static final long serialVersionUID = 1L;
	
	public PatientBronch() {
		bronchAutoNumber = -1;
	}
	
	private int bronchAutoNumber;
	private int bronchType;
	private String locationCode;
	private boolean rose;
	private boolean lymphocites;
	private String biopsyResult;
	private String comment;
	private String diagnostic;
	private String accuracy;
	private int numberPasses;
	private String culture;
	private int biopsyNumber;

	
	public int getBronchAutoNumber() {
		return this.bronchAutoNumber;
	}
	
	public void setBronchAutoNumber(int bronchAutoNumber) {
		this.bronchAutoNumber = bronchAutoNumber;
	}
	
	public int getBronchType() {
		return bronchType;
	}
	
	public void setBronchType(int bronchType) {
		this.bronchType = bronchType;
	}
	
	public String getLocation() {
		return this.locationCode;
	}
	
	public void setLocation(String location) {
		this.locationCode = location;
	}
	
	public boolean getRose() {
		return this.rose;
	}
	
	public void setRose(boolean rose) {
		this.rose = rose;
	}
	
	public boolean getLymphocites() {
		return this.lymphocites;
	}
	
	public void setLymphocites(boolean lymphocites) {
		this.lymphocites = lymphocites;
	}
	
	public String getBiopsyResult() {
		return this.biopsyResult;
	}
	
	public void setBiopsyResult(String biopsyResult) {
		this.biopsyResult = biopsyResult;
	}
	
	public String getComment() {
		return comment;
	}
	
	public void setComment(String comment) {
		this.comment = comment;
	}
	
	/*
	 * private String diagnostic;
	private String accuracy;
	private int numberPasses;
	private String culture;
	private int biopsyNumber;
	 * */
	public String getDiagnostic() {
		return this.diagnostic;
	}
	
	public void setDisgnostic(String diagnostic) {
		this.diagnostic = diagnostic;
	}
	
	public String getAccuracy() {
		return this.accuracy;
	}
	
	public void setAccuracy(String accuracy) {
		this.accuracy = accuracy;
	}
	
	public int getNumberPasses() {
		return this.numberPasses;
	}
	
	public void setNumberPasses(int numberPasses) {
		this.numberPasses = numberPasses;
	}
	
	public String getCulture() {
		return this.culture;
	}
	
	public void setCulture(String culture) {
		this.culture = culture;
	}
	
	public int getBiopsyNumber() {
		return this.biopsyNumber;
	}
	
	public void setBiopsyNumber(int biopsyNumber) {
		this.biopsyNumber = biopsyNumber;
	}
}
